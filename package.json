    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^5.1.2",
    "nodemon": "^1.17.5",
    "typescript": "3.2.4"
  },
