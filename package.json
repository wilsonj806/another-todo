{
  "name": "todolet",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsonj806/todolet.git"
  },
  "scripts": {
    "heroku-prebuild": "npm i --only=dev && npm i && npm run build && cd client && npm i && cd ../ && echo TypeScript build successful && cd client && npm run build && npm prune && cd ../ && echo React build success && npm prune && echo Build successful, packages pruned",
    "build": "tsc",
    "start": "node dist/server.js",
    "server": "nodemon --exec babel-node --extensions '.ts' server.ts ",
    "client": "npm start --prefix client",
    "watch-ts": "tsc --watch",
    "lint": "eslint ./**/*.ts",
    "test": "jest --config=jest.config.js --watch",
    "test-client": "jest --config=react.jest.config.js --watch",
    "coverage": "jest --coverage --config=jest.config.js && jest --coverage --config=react.jest.config.js && mkdir coverage && touch coverage/lcov.info && lcov-result-merger 'todolet_coverage_*/lcov.info' './coverage/lcov.info' && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./todolet_coverage_* "
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "concurrently": "3.6.0",
    "connect-flash": "0.1.1",
    "connect-mongodb-session": "2.1.1",
    "cors": "^2.8.5",
    "dotenv": "8.0.0",
    "express": "4.16.4",
    "express-messages": "1.0.1",
    "express-session": "1.16.1",
    "express-validator": "5.3.1",
    "mongoose": "5.5.7",
    "mongoose-type-url": "1.0.6",
    "passport": "0.4.0",
    "passport-local": "1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@shelf/jest-mongodb": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-mongodb-session": "^0.0.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/jest": "^23.3.14",
    "@types/mongodb": "^3.1.26",
    "@types/mongoose": "^5.3.27",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^5.1.2",
    "jest": "24.7.1",
    "lcov-result-merger": "^3.1.0",
    "nodemon": "^1.17.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "3.2.4"
  },
  "engines": {
    "node": "10.10.0"
  }
}
