  },
  "scripts": {
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies. && npm run build",
    "build": "tsc",
    "start": "node dist/server.js",
    "deploy": "gh-pages -d client/build",
    "server": "nodemon dist/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "watch-ts": "tsc --watch",
    "lint": "eslint ./**/*.ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "concurrently": "3.6.0",
    "connect-flash": "0.1.1",
    "connect-mongodb-session": "2.1.1",
    "dotenv": "8.0.0",
    "express": "4.16.4",
    "express-messages": "1.0.1",
    "express-session": "1.16.1",
    "express-validator": "5.3.1",
    "mongoose": "5.5.7",
    "mongoose-type-url": "1.0.6",
    "passport": "0.4.0",
    "passport-local": "1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-mongodb-session": "0.0.0",
    "@types/express": "^4.16.1",
